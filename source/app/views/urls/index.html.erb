<h1>URL List</h1>
 
<table>
  <tr>
    <th>Short URL</th>
    <th>Full URL</th>
    <th>Click Count</th>
  </tr>
 
  <% @urls.each do |url| %>
    <tr>
      <td><a href="http://localhost:3000/urls/<%= url.url_code %>">http://localhost:3000/urls/<%= url.url_code %></a></td>
      <td><a href="<%= url.full_url %>"><%= url.full_url %></a></td>
      <td><%= url.click_count %></td>
    </tr>
  <% end %>

</table>
<br>
<%= button_to "New", '/urls/new', method: :get %>
